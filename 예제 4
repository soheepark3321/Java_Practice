package CH02_Variable;
import java.util.*;

public class ex2_1 {

	public static void main(String[] args) {
		int sum = 0;
		
		for(int i=1; i<=20; i++) {
			//System.out.println(i);
			if(!((i%2==0)||(i%3==0))) {
				//System.out.println(i);
				sum = sum + i;
				System.out.println(sum);
			}
		}
	} 


}
