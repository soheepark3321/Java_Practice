		for(int i=1; i<=5; i++) {
			for(int j=1; j<=5; j++) {
				if(i==j) {
					//System.out.printf("[%d,%d]", i, j);
					System.out.printf("*", i, j);
				} else {
					System.out.printf("%5c", ' ');
				}
		}
			System.out.println();

	}

